@model IEnumerable<MKT.Entities.Models.CartItem>

@{ ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml"; }

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cart.Usuario.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(model => item.Cart.Usuario.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Product.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>

        </tr>
    }
    
    </table>
</body>
</html>
