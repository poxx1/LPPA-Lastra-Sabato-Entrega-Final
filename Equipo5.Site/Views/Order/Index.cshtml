@model IEnumerable<MKT.Entities.Models.Order>

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario.NombreWeb)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) 
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.NombreWeb)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
